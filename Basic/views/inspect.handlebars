<div class="container-fluid dashboard">
    <div class="row main-content">
        <div class="col-sm-2 letter-color background">
            <div class="row nav-section">
                <div class="col-sm-12 logo">
                    <img src="../../assets/images/logo.png" width="150" height="100" alt="Logo">
                </div>
                <div class="col-sm-12 nav-content">

                    <a class="nav-links overview" href="/home/{{route}}">
                        <img src="../../assets/images/view_icon.png" alt="Overview" class="nav-icon">
                        <div class="link-name letter-color" style="display:none">Overview</div>
                    </a>
                    <a class="nav-links units" href="/units/{{route}}">
                        <img src="../../assets/images/home_icon.png" alt="Home" class="nav-icon">
                        <div class="link-name letter-color" style="display:none">Units</div>
                    </a>
                    <a class="nav-links inspection" href="/inspection/{{route}}">
                        <img src="../../assets/images/mag_glass.png" alt="Magnifying Glass" class="nav-icon">
                        <div class="link-name letter-color" style="display:none">Inspections</div>
                    </a>
                    <a class="nav-links template" href="/templates/{{route}}">
                        <img src="../../assets/images/temp_icon.png" alt="Template" class="nav-icon">
                        <div class="link-name letter-color" style="display:none">Templates</div>
                    </a>
                    <a class="nav-links report" href="/reports/{{route}}">
                        <img src="../../assets/images/rep_icon.png" alt="Report" class="nav-icon">
                        <div class="link-name letter-color" style="display:none">Reports</div>
                    </a>
                </div>
                <div class="col-sm-12 company-info">
                    Hometeam Property Management
                </div>
            </div>
        </div>

        <div class="col-sm-10 main">
            <div class="row homeT">
                <div class="col-sm-9">
                    {{#each unit}}
                    <div>
                        <div>ID: <span class="">{{unit_id}}</span></div>
                        <div>ADDRESS: {{street}}</div>
                        <div>CITY: {{city}} STATE: {{state}} ZIP:{{zip}}</div>
                    </div>
                    {{/each}}

                        <div class="inspectDetails">
                            {{#each fields}}
                                <div class="dropdownBtns">
                                    <p>
                                        <button id="temp-btn-entry" class="btn" type="button" data-toggle="collapse" data-target="#{{title}}" aria-expanded="true"
                                            aria-controls="collapseExample">
                                            <img src="./public/assets/images/dropdown.png" alt="#" height="20px" width="20px">
                                            <div id="temp-label-entry"> {{title}} </div>
                                        </button>
                                    </p>
                                    <div class="collapse" id="{{title}}">
                                        {{#each sections}}
                                        <div class="card card-body">
                                            <div>Title: <span>{{title}}</span></div>
                                            <div>Clean: 
                                                    <input type="radio" id="contactChoice1" name="contact" value="email">
                                            </div>
                                            <div>Undamaged: 
                                                    <input type="radio" id="contactChoice1" name="contact" value="email">
                                            </div>
                                            <div>Working: 
                                                    <input type="radio" id="contactChoice1" name="contact" value="email">
                                            </div>
                                            <div class="detail-icons">
                                                    <input id="image" type="image" alt="inputPicture" src="http://www.clker.com/cliparts/i/l/9/W/l/m/camera-icon-hi.png" style="width:40px;height:40px;" accept="image/gif, image/jpeg, image/png">
                                                    <img src="http://www.free-icons-download.net/images/save-a-web-page-button-73094.png" alt="save" style="width:40px;height:40px;">
                                            </div>
                                        </div>
                                        {{/each}}
                                    </div>
                                </div>
                            {{/each}}
                        </div>

                </div>
            </div>
        </div>
    </div>
</div>