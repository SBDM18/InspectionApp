<div class="row dash-main">
    <div id="myCarousel" class="carousel slide" data-ride="carousel">
        <div class="carousel-inner row w-100 mx-auto">
            <div class="carousel-item col-md-4 active ">
                <div class="card inspecCardD" data-id="Completed">                    
                    <div class="card-body" style="background-color:yellowgreen;">
                        <h4 class="card-title" >Complete</h4>
                        <p class="card-text">Amount of Inspections completed: {{complete}}</p>
                        <p class="card-text">
                            <small class="text-muted">Last updated 3 mins ago</small>
                        </p>
                    </div>
                </div>
            </div>
            <div class="carousel-item col-md-4">
                <div class="card inspecCardD" data-id="In Progress">                    
                    <div class="card-body" style="background-color:rgb(201, 15, 167);">
                        <h4 class="card-title">In Progress</h4>
                        <p class="card-text">Amount of Inspections incomplete: {{incomplete}}</p>
                        <p class="card-text">
                            <small class="text-muted">Last updated 3 mins ago</small>
                        </p>
                    </div>
                </div>
            </div>
            <div class="carousel-item col-md-4">
                <div class="card inspecCardD" data-id="Incomplete">                   
                    <div class="card-body" style="background-color:red;">
                        <h4 class="card-title">Incomplete</h4>
                        <p class="card-text">Amount of Inspections inprogress: {{inprogress}}</p>
                        <p class="card-text">
                            <small class="text-muted">Last updated 3 mins ago</small>
                        </p>
                    </div>
                </div>
            </div>
            <div class="carousel-item col-md-4">
                <div class="card inspecCardD" data-id="Total">
                    <div class="card-body" style="background-color:rgb(0, 195, 255);">
                        <h4 class="card-title">Total</h4>
                        <p class="card-text">Total number of Inspections: {{total}}</p>
                        <p class="card-text">
                            <small class="text-muted">Last updated 3 mins ago</small>
                        </p>
                    </div>
                </div>
            </div>
            
        </div>
        <a class="carousel-control-prev" href="#myCarousel" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#myCarousel" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
     <div class="inspTab-cont">
        <table class="table table-striped table-hover inspectTab" style="background-color:white;">
            <thead class="thead-light">
                <tr>
                    <th scope="col">Address</th>
                    <th scope="col">City</th>
                    <th scope="col">Type</th>
                    <th scope="col">Date</th>
                    <th scope="col">Status</th>
                    <th scope="col">Created</th>
                </tr>
            </thead>
            <tr>
                <tbody >
                    {{#each status}}
                        <tr>
                            <td style="font-size 6px;">{{street}}</td>
                            <td style="font-size 6px;">{{city}}</td>
                            <td style="font-size 6px;">Type</td>
                            <td style="font-size 6px;">{{created}}</td>
                            <td style="font-size 6px;">User</td>
                            <td style="font-size 6px;">{{status}}</td>
                        </tr>
                    {{/each}}
            </tbody>
        </table>
        </div>
    </div>